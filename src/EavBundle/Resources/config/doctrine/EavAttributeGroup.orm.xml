<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EavBundle\Entity\EavAttributeGroup" table="eav_attribute_group">
    <indexes>
      <index name="user_id" columns="actioneer"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="group" type="string" column="group" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="access" type="text" column="access" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="actioneer" type="integer" column="actioneer" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false"/>
    <field name="modified" type="datetime" column="modified" nullable="false"/>
  </entity>
</doctrine-mapping>
